{
    "name": "@fqlan/node-addon-example-prebuildify",
    "version": "0.0.2",
    "description": "Hello world app packaged with node-gyp-build",
    "main": "./index.js",
    "author": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/lanfungcing/node-addon-example-prebuildify.git"
    },
    "files": [
        "binding.gyp",
        "index.js",
        "src/hello.c",
        "prebuilds"
    ],
    "dependencies": {
        "node-gyp-build": "^4.0.0",
        "node-gyp": "^11.3.0"
    },
    "devDependencies": {
        "prebuildify": "^6.0.1"
    },
    "scripts": {
        "install": "node-gyp-build",
        "prebuild": "prebuildify --napi --strip --target=22.19.0",
        "prebuild-darwin-x64+arm64": "prebuildify --arch x64+arm64 --napi --strip --target=22.19.0",
        "test": "node index.js"
    }
}
