    "devDependencies": {
        "prebuildify": "^6.0.1"
    },
    "scripts": {
        "install": "node-gyp-build",
        "prebuild": "prebuildify --napi --strip",
        "prebuild-darwin-x64+arm64": "prebuildify --arch x64+arm64 --napi --strip",
        "test": "node index.js"
    }
}
